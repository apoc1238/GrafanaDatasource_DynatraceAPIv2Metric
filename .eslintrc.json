{
    "extends": "airbnb-base",
    "rules": {
      "no-console": "off",
      "no-param-reassign": [2, { "props": false }],
      "no-underscore-dangle": 0,
      "import/no-unresolved": 0,
      "import/no-extraneous-dependencies": 0,
      "import/no-webpack-loader-syntax": 0,
      "import/extensions": 0
    }
}
